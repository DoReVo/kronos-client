{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "compilerOptions": {
    "baseUrl": ".",
    "strictNullChecks": true,
    "types": [
      "vite-plugin-vue-layouts/client",
      "vite-plugin-pages/client"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.vue"
  ],
  "exclude": [
    "dist",
    "node_modules",
  ],
  "references": [
    {
      "path": "./tsconfig.vite-config.json"
    }
  ]
}