{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "compilerOptions": {
    "noImplicitAny": false,
    "baseUrl": ".",
    "strictNullChecks": true,
    "types": [
      "vite-plugin-vue-layouts/client",
      "vite-plugin-pages/client"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "references": [
    {
      "path": "./tsconfig.vite-config.json"
    }
  ]
}